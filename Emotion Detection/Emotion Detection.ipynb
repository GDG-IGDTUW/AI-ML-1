    "'''create a word clouds for each emotion category'''\n",
    "from wordcloud import WordCloud\n",
    "\n",
    "grouped_data = df.groupby(\"Emotion\")[\"Clean_Text\"].apply(lambda texts: \" \".join(texts))\n",
    "\n",
    "#word cloud for each emotion\n",
    "fig, axes = plt.subplots(1, len(grouped_data), figsize=(15, 5))\n",
    "\n",
    "for i, (emotion, text) in enumerate(grouped_data.items()):\n",
    "    wordcloud = WordCloud(width=800, height=400, background_color=\"white\").generate(text)\n",
    "    axes[i].imshow(wordcloud, interpolation=\"bilinear\")\n",
    "    axes[i].set_title(emotion.capitalize(), fontsize=14)\n",
    "    axes[i].axis(\"off\")\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "'''visualize the confusion matrix with labels and a color gradient'''\n",
    "\n",
    "y_pred = pipe_lr.predict(x_test)\n",
    "\n",
    "emotions = pipe_lr.classes_\n",
    "conf_matrix = confusion_matrix(y_test, y_pred, labels=emotions)\n",
    "\n",
    "#normalizing\n",
    "conf_matrix_normalized = conf_matrix.astype('float') / conf_matrix.sum(axis=1, keepdims=True)\n",
    "\n",
    "#visualizing\n",
    "plt.figure(figsize=(12, 8))\n",
    "sns.heatmap(conf_matrix_normalized, annot=True, fmt=\".2f\", cmap=\"Blues\", \n",
    "            xticklabels=emotions, yticklabels=emotions)\n",
    "\n",
